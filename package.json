{
  "name": "redux-scripts-manager",
  "version": "0.3.0",
  "description": "Manage dynamic script loading with Redux",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src --source-maps --out-dir ./dist",
    "clean": "GLOBIGNORE=.gitignore:.npmignore rm -rf ./dist/*",
    "cover": "NODE_ENV=test nyc --reporter=lcov --reporter=text npm test -- --reporter min",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint ./src ./test",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags",
    "start": "npm run clean && npm run build",
    "test": "mocha",
    "watch": "npm run clean && babel ./src --source-maps --watch --out-dir ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhardy/redux-scripts-manager.git"
  },
  "keywords": [
    "redux",
    "scripts",
    "manager",
    "dynamic",
    "script",
    "loading"
  ],
  "author": "Nathan Hardy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nhardy/redux-scripts-manager/issues"
  },
  "homepage": "https://github.com/nhardy/redux-scripts-manager#readme",
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-config-nhardy": "^0.3.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "proxyquire": "^1.8.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.14.0"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "sha1": "^1.1.1"
  },
  "peerDependencies": {
    "redux": "*",
    "redux-thunk": "*"
  }
}
